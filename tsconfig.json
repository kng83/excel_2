  "compilerOptions": {
    "esModuleInterop": true,
    "declaration": false,
    "outDir": "./dist/out-files",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "exclude":[
    "server"
  ]
}
